<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ev3"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/Contenedor"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:tag="@string/app_name"
        tools:context=".wedo">

        <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="1dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TableRow
                        android:id="@+id/text1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:backgroundTint="#FFFFFF">

                        <TextView
                            android:id="@+id/textView6"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:text="Carreras"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:text="Futbol"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView8"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:text="Boxeo"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                    </TableRow>

                    <TableRow
                        android:id="@+id/primera"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="100"
                        android:clickable="false"
                        android:minHeight="100dp">

                        <ImageButton
                            android:id="@+id/B_carreras"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:src="@drawable/carreras"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />

                        <ImageButton
                            android:id="@+id/B_futbol"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:src="@drawable/futbol"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />

                        <ImageButton
                            android:id="@+id/B_Boxeo"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:src="@drawable/boxeador_ev3"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />
                    </TableRow>

                    <TableRow
                        android:id="@+id/text2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:text="Dino"
                            android:textAlignment="center"
                            android:textAllCaps="false"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:textAlignment="center"
                            android:textAllCaps="false"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView12"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:textAlignment="center"
                            android:textAllCaps="false"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageButton
                            android:id="@+id/B_dino"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:src="@drawable/dino"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />

                        <ImageButton
                            android:id="@+id/b_Quitanieves5"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />

                        <ImageButton
                            android:id="@+id/b_Quitanieves6"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />
                    </TableRow>

                    <TableRow
                        android:id="@+id/texto3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/textView"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageButton
                            android:id="@+id/b_Quitanieves7"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />

                        <ImageButton
                            android:id="@+id/b_Quitanieves8"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />

                        <ImageButton
                            android:id="@+id/b_Quitanieves9"
                            android:layout_width="0dp"
                            android:layout_height="250dp"
                            android:layout_weight="1"
                            android:backgroundTint="@android:color/transparent"
                            android:contentDescription="@string/Helicoptero"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:onClick="seleccion"
                            android:scaleType="fitCenter"
                            android:textAlignment="gravity"
                            android:textDirection="firstStrongRtl" />
                    </TableRow>

                    <TableRow
                        android:id="@+id/texto4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                        <TextView
                            android:id="@+id/textView9"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@android:color/darker_gray"
                            android:contentDescription="@string/Vacio"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            tools:ignore="TextContrastCheck" />

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>

                </TableLayout>
            </LinearLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>